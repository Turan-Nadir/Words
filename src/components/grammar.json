    "grammar_points": [
          {
            "pattern": "NOUN + -은/는",
            "type": "topic marking particles",
            "examples": [
              "저희 집에는 = at our house",
              "회사에 있다 = to be at work"
            ]
          },
          {
            "pattern": "NOUN + -에",
            "type": "location particles",
            "examples": [
              "의자에 앉다 = to sit on a chair",
              "쓰레기통에 버리다 = to throw away in a trash bin"
            ]
          },
          {
